<% flash.each do |message_type, message| %>
  <div class="alert alert-<%= message_type %>"><%= message %></div>
<% end %>

<h1>Log in</h1>

<%= form_for(:session, url: signin_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
 
  <%= f.label :password %>
  <%= f.password_field :password %>
 
  <%= f.submit "Sign In" %>
<% end %>